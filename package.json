{
  "name": "vuelve",
  "description": "A declarative syntax for the Composition API in Vue 3.",
  "version": "0.2.1",
  "type": "module",
  "main": "dist/vuelve.cjs.js",
  "module": "dist/vuelve.esm.js",
  "types": "dist/vuelve.d.ts",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "lint-staged",
    "lint-all": "pretty-quick --pattern \"src/**/*.js\" --pattern \"test/**/*.js\" && eslint --fix src",
    "test": "jest --coverage",
    "test-watch": "jest --watch --coverage"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dashersw/vuelve.git"
  },
  "author": "Armagan Amcalar <armagan@amcalar.com>",
  "bugs": {
    "url": "https://github.com/dashersw/vuelve/issues"
  },
  "homepage": "https://github.com/dashersw/vuelve#readme",
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "lodash.clonedeep": "^4.5.0"
  },
  "devDependencies": {
    "@vue/test-utils": "^2.0.0-beta.7",
    "coveralls": "^3.1.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^10.5.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "vite-plugin-dts": "^3.7.3",
    "vue": "^3.0.2",
    "vite": "^5.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "lint-staged": {
    "src/*.js": [
      "pretty-quick --staged",
      "eslint --fix"
    ]
  }
}
